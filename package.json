{
  "name": "caphub-server-main",
  "version": "0.0.1",
  "scripts": {
    "build": "npx ts-node build.ts",
    "lint": "eslint . --ext .ts",
    "prestart": "npm run build",
    "start": "node ./dist --env=production",
    "predev": "npm install",
    "dev": "nodemon",
    "prebuild": "npm run version:patch",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "./node_modules/.bin/ts-node ./src"
  },
  "dependencies": {
    "@caphub-funding/caphub-types": "^1.0.7",
    "@sendgrid/mail": "^7.7.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/docusign-esign": "^5.19.0",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.7",
    "@types/passport-facebook": "^2.1.11",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-linkedin-oauth2": "^1.5.3",
    "@types/passport-twitter": "^1.0.37",
    "@types/uuid": "^9.0.1",
    "@types/zxcvbn": "^4.4.1",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "docusign-esign": "^6.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "http-proxy-middleware": "^2.0.6",
    "install": "^0.13.0",
    "jet-logger": "^1.3.1",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.3",
    "multer": "^1.4.5-lts.1",
    "npm": "^9.6.4",
    "passport": "^0.6.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-linkedin-oauth2": "^2.0.0",
    "passport-twitter": "^1.0.4",
    "typescript": "^5.0.4",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1"
  }
}
